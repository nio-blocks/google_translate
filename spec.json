{
  "nio/GoogleTranslate": {
    "version": "0.1.0",
    "description": "Translate text to any language with Google Translate",
    "properties": {
      "dest": {
        "title": "Destination Language",
        "type": "StringType",
        "description": "Language code of destination language",
        "default": null
      },
      "src": {
        "title": "Source Language",
        "type": "StringType",
        "description": "Language code of source language",
        "default": null
      }
    },
    "inputs": {
      "default": {
        "description": "A signal containing a 'text' attribute to be translated"
      }
    },
    "outputs": {
      "default": {
        "description": "A signal with the text value translated to the destination language"
      }
    },
    "commands": {}
  }
}
